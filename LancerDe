import random

class LanceDe:
    def __init__(self):
        self.critique=20
        self.normal=15
        self.presque=10
        self.rate=0

    def LanceDe(self):
        i=random.randint(0,21)
        if 15<i:
            return self.critique
        elif 10<i<15:
            return self.normal
        elif 5<i<10:
            return self.presque
        else:
            return self.rate
